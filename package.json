{
  "name": "phaser3-multiplayer-example-with-geckos.io",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel parcel:serve server:serve",
    "build": "npm run parcel:build",
    "start": "NODE_ENV=production node server/server.js",
    "test": "node test/test.js",
