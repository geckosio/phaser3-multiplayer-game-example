    "parcel:build": "parcel build index.html --no-source-maps",
    "parcel:serve": "parcel serve index.html --no-source-maps",
    "server:serve": "nodemon --delay 500ms server/server.js"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "@geckos.io/client": "^1.1.8",
    "@geckos.io/phaser-on-nodejs": "^1.0.3",
    "@geckos.io/server": "^1.1.8",
    "axios": "^0.18.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "phaser": "^3.20.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "parcel": "^1.12.4",
    "parcel-plugin-static-files-copy": "^2.2.1",
    "puppeteer": "^2.0.0"
